{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri_version",
  "version": "0.1.0",
  "identifier": "com.ash.tauri-version",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "VR Club Catalog",
        "width": 1280,
        "height": 720,
        "fullscreen": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": "default-src 'self' 'unsafe-inline' 'unsafe-eval' asset: http: https: file: data: blob: media-src 'self' asset: http: https: file: data: blob:;"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "opener": {}
  }
}